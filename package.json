{
  "name": "jupiverse-kit",
  "version": "1.2.0",
  "description": "Jupiverse Kit is the ultimate ready-to-use React components library powered by Jupiter's APIs for building onchain applications on Solana effortlessly.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "dist/index.css",
    "dist/assets"
  ],
  "scripts": {
    "copy-assets": "cp -r src/assets dist/",
    "build": "rollup -c && npm run copy-assets",
    "dev": "rollup -c -w",
    "clean": "rimraf dist",
    "prepare": "npm run clean && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
